    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.9",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms": "^2.3.2",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.9.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "reason-react": "^0.5.3",
    "uuid": "^3.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "start": "yarn develop",
    "clean": "rimraf .cache public",
    "build:gatsby": "yarn clean && gatsby build",
    "build": "yarn clean:bs && yarn clean && yarn build:bs && gatsby build",
    "develop": "yarn clean && gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"{gatsby-*.js,src/**/*.js}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bsb": "bsb",
    "build:bs": "bsb -make-world",
    "start:bs": "bsb -make-world -w",
    "clean:bs": "bsb -clean-world",
    "test:bs": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack -w",
    "webpack:production": "NODE_ENV=production webpack",
    "webpack:server": "webpack-dev-server",
    "lint": "yarn eslint *.js src"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "babel-eslint": "^10.0.1",
    "bs-platform": "^4.0.18",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "gatsby-plugin-eslint": "^2.0.3",
    "husky": "^1.3.1",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn build:bs",
      "pre-commit": "exit 0",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "...": "..."
    }
  }
}
